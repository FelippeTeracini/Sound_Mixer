            }
            else if (subUsers[h].s === 10) {
                $currUser.find('.user-management-status').addClass('pending');
                $currUserLeftPane.find('.user-management-status').removeClass('enabled disabled')
                    .addClass('pending');
                colorBg = true;
            }
            else {
